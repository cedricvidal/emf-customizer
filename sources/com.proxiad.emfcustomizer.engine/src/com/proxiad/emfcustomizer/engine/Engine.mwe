<workflow>
	<component class="org.eclipse.xtext.MweReader" uri="${modelFile}">
		<!-- this class will be generated by the xtext generator -->
		<register class="com.proxiad.emfcustomizer.stylesheet.dsl.StylesheetStandaloneSetup"/>
		<outputSlot value='cssModel'/>
	</component>
	<component class='com.proxiad.emfcustomizer.engine.EmfCustomizerComponent'>
		<cssSlot value='cssModel'/>
		<outputSlot value='${outputSlot}'/>
	</component>
</workflow>
